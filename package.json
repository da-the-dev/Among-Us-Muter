    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "16.6.1"
  }
